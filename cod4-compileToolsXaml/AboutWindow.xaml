        mc:Ignorable="d"
        Title="About cod4-compileToolsXaml" Height="149.5" Width="400" MinHeight="140" MinWidth="400" WindowStartupLocation="CenterOwner"
        ResizeMode="NoResize" WindowStyle="ToolWindow">
