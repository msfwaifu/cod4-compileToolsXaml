        <Label x:Name="VersionLabel" Content="Version: {0}" HorizontalAlignment="Left" Margin="223,10,0,0" VerticalAlignment="Top" Height="27" Width="130" Background="{x:Null}" Foreground="White"/>
        <Button Style="{StaticResource ToolButton}" x:Name="HomePageButton" Click="HomePageButtonClick" Content="Github Repo" HorizontalAlignment="Left" Margin="30,0,0,10" Width="160" Height="27" VerticalAlignment="Bottom"/>
        <Button Style="{StaticResource ToolButton}" x:Name="BouncepatchButton" Click="BouncepatchButtonClick" Content="xoxor4d.github.io" HorizontalAlignment="Right" Margin="0,0,30,10" Width="160" Height="27" VerticalAlignment="Bottom"/>
